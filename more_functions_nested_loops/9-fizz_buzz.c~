#include "main.h"
/**
 * print_fizz_buzz :print_square - prints square
 *
 */

void print_fizz_buzz(void)
{
int i;
for (i = 1 ; i <= 100 ; i++)
{
if (i % 3 == 0 && i % 5 == 0)
printf("FizzBuzz ");
else if (i % 3 == 0)
printf("Fizz ");
else if (i % 5 == 0)
printf("Buzz ");
else
printf("%d ", i);
}
_putchar('\n');
}
